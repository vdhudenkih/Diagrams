некорректный вывод:
ожидалось:
wordOrder: 0, wordText: набор, wordId: 166595, partOfSpeech: NOUN, wordKind: masc, singPlur: sing, wordCase: nomn
wordOrder: 1, wordText: сварочный, wordId: 314735, partOfSpeech: ADJF, wordKind: masc, singPlur: plur, wordCase: gent
wordOrder: 2, wordText: аппарат, wordId: 15358, partOfSpeech: NOUN, wordKind: masc, singPlur: plur, wordCase: gent
получено:
wordOrder: 0, wordText: набор, wordId: 166595, partOfSpeech: NOUN, wordKind: masc, singPlur: sing, wordCase: nomn
wordOrder: 1, wordText: сварочный, wordId: 314735, partOfSpeech: ADJF, wordKind: masc, singPlur: sing, wordCase: nomn
wordOrder: 2, wordText: аппарат, wordId: 15358, partOfSpeech: NOUN, wordKind: masc, singPlur: sing, wordCase: nomn



напиши функцию на ЯП c# по следующему условию:
на вход подаётся List<Word> words
программа считывает все объекты класса Word с параметрами:
public int wordOrder;
public string wordText;
public string wordId;
public string partOfSpeech;
public string wordKind;
public string singPlur;
public string wordCase;
типичным объектом будет, например "0, кошка, 1000, NOUN, femn, nomn"
Тебе нужно изменить параметры объектов по следующей логике:
Если параметр объекта partOfSpeech=ADJF, то ничего не делать
Если параметр одного объекта partOfSpeech=NOUN и нет объекта где (wordText="для" или "от" или "из" или "к" или "с" или "в" или "на" или "под" или "через")
то:{
    если нет других объектов с параметром partOfSpeech=NOUN 
    то:{
        изменить (параметры wordKind и wordCase на равные аналогичным у объекта с параметром NOUN) у всех объектов, где параметр partOfSpeech=ADJF 
        }
    иначе:
        {
        Если нет объектов с параметром ADJF:
        то:
            {
                если не (объект с параметром wordOrder==0 имеет параметр wordText= "набор" или "комплект" или "каталог" или "блок")
                то:{
                    изменить параметры wordCase всех объектов, кроме объекта с параметром wordOrder==0, на wordCase="gent"
                    }
                иначе:
                    {
                    изменить параметры (wordKind и wordCase и singPlur всех объектов, кроме объекта с параметром wordOrder==0), на (wordKind="masc" и wordCase="gent" и singPlur="plur")
                    }
            }
        Иначе:
        {
            если (объект с параметром wordOrder==0 имеет параметр wordText= "набор" или "комплект" или "каталог" или "блок")
            то:
            {
                изменить параметры (wordKind и wordCase и singPlur всех объектов, кроме объекта с параметром wordOrder==0), на  wordKind="masc" и wordCase="gent" и singPlur="plur"
            }
            иначе:
            {
                изменить параметры (wordKind и wordCase и singPlur всех объектов, кроме объекта с параметром wordOrder==0), на  равные объекту с параметром wordOrder==0
                изменить параметр (wordCase всех объектов, кроме объекта с параметром wordOrder==0), на wordCase="gent"
            }
        }
        }
}







using System;
using System.Collections.Generic;
using System.Linq;

public class WordProcessor
{
    public static void ModifyWords(List<Word> words)
    {
        if (words == null || words.Count == 0)
            return;

        // Список предлогов для проверки
        var prepositions = new HashSet<string> { "для", "от", "из", "к", "с", "в", "на", "под", "через" };

        // 1. Если есть объект с partOfSpeech == ADJF, то ничего с ним не делать
        // (т.е. мы не меняем ADJF напрямую, но в логике ниже меняем их параметры в некоторых случаях)

        // Проверяем наличие объекта с partOfSpeech == NOUN
        var nouns = words.Where(w => w.partOfSpeech == "NOUN").ToList();

        // Проверяем наличие предлогов в списке
        bool containsPreposition = words.Any(w => prepositions.Contains(w.wordText));

        if (!containsPreposition && nouns.Any())
        {
            // Если есть NOUN и нет предлогов, идём дальше

            // Проверяем, есть ли других NOUN кроме текущего
            if (nouns.Count == 1)
            {
                // Если нет других объектов с partOfSpeech= NOUN (т.е. ровно 1 NOUN)

                // Нужно изменить wordKind и wordCase у всех ADJF на аналогичные у объекта с partOfSpeech= NOUN

                var noun = nouns.First();

                foreach (var w in words)
                {
                    if (w.partOfSpeech == "ADJF")
                    {
                        w.wordKind = noun.wordKind;
                        w.wordCase = noun.wordCase;
                    }
                }
            }
            else
            {
                // Иначе (т.е. NOUN больше одного)

                // Проверяем есть ли объекты с partOfSpeech == ADJF
                var adjs = words.Where(w => w.partOfSpeech == "ADJF").ToList();

                // Получаем объект с wordOrder==0
                var firstWord = words.FirstOrDefault(w => w.wordOrder == 0);

                bool firstWordIsSpecial = firstWord != null && 
                    (firstWord.wordText == "набор" || firstWord.wordText == "комплект" || firstWord.wordText == "каталог" || firstWord.wordText == "блок");

                if (!adjs.Any())
                {
                    // Если нет ADJF

                    if (firstWord == null || !firstWordIsSpecial)
                    {
                        // если не (объект с wordOrder==0 имеет wordText из списка)
                        // изменить wordCase всех объектов, кроме wordOrder==0, на "gent"

                        foreach (var w in words)
                        {
                            if (w.wordOrder != 0)
                            {
                                w.wordCase = "gent";
                            }
                        }
                    }
                    else
                    {
                        // иначе
                        // изменить wordKind, wordCase, singPlur всех объектов, кроме wordOrder==0
                        // на wordKind="masc", wordCase="gent", singPlur="plur"

                        foreach (var w in words)
                        {
                            if (w.wordOrder != 0)
                            {
                                w.wordKind = "masc";
                                w.wordCase = "gent";
                                w.singPlur = "plur";
                            }
                        }
                    }
                }
                else
                {
                    // Иначе (есть ADJF)

                    if (firstWordIsSpecial)
                    {
                        // если объект с wordOrder==0 имеет wordText из списка
                        // изменить wordKind, wordCase, singPlur всех объектов, кроме wordOrder==0
                        // на wordKind="masc", wordCase="gent", singPlur="plur"

                        foreach (var w in words)
                        {
                            if (w.wordOrder != 0)
                            {
                                w.wordKind = "masc";
                                w.wordCase = "gent";
                                w.singPlur = "plur";
                            }
                        }
                    }
                    else
                    {
                        // иначе
                        // изменить wordKind, wordCase, singPlur всех объектов, кроме wordOrder==0, на равные объекту с wordOrder==0
                        // затем изменить wordCase всех объектов, кроме wordOrder==0, на "gent"

                        if (firstWord != null)
                        {
                            foreach (var w in words)
                            {
                                if (w.wordOrder != 0)
                                {
                                    w.wordKind = firstWord.wordKind;
                                    w.wordCase = firstWord.wordCase;
                                    w.singPlur = firstWord.singPlur;
                                }
                            }

                            foreach (var w in words)
                            {
                                if (w.wordOrder != 0)
                                {
                                    w.wordCase = "gent";
                                }
                            }
                        }
                    }
                }
            }
        }
        // Если partOfSpeech == ADJF — ничего не делать (уже учтено, мы меняем их параметры только при необходимости выше)
        // Если partOfSpeech == NOUN и есть предлоги — ничего делать не надо (т.к. условие обрабатывается только если предлогов нет)
    }
}










